{
  "manifest_version": 2,
  "name": "Stretch Reminder",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
  ],
  "browser_action": {
        "default_title": "Stretch Reminder",
        "default_popup": "popup.html"
    }
}

